{
  "name": "paranoia",
  "version": "0.1.0",
  "description": "A parser for already parsed JSON",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/larohr/paranoia.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:ts && npm run build:copy",
    "build:ts": "tsc -p tsconfig.json && tsc -p tsconfig.json -m esnext --outDir dist/esm/ -d false -declarationMap false",
    "build:copy": "copyfiles README.md LICENSE src/package.json dist --flat",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/jest": "^29.1.2",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "jest": "^29.1.2",
    "runmd": "^1.3.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "ts-results": "^3.3.0",
    "tslib": "^2.4.0"
  }
}
